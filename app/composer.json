{
    "name": "precios-gamer/api-rest",
    "description": "Servicio de apir rest usando PHP y Slim 4.",
    "keywords": [
        "php",
        "slim-micro-framework",
        "rest-api",
        "slim4",
        "slim",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/LibreOpcion/base-slim-4-api-rest",
    "license": "MIT",
    "authors": [
        {
            "name": "Ender Puentes",
            "email": "epuentes@libreopcion.com",
            "homepage": "https://github.com/EnderPuentes"
        }
    ],
    "config": {
        "platform": {
            "php": "8.0"
        },
        "sort-packages": true,
        "process-timeout": 0
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "cakephp/validation": "^4.2",
        "firebase/php-jwt": "^5.3",
        "fzaninotto/faker": "^1.9",
        "mercadopago/dx-php": "^2.4",
        "monolog/monolog": "^2.2",
        "panique/pdo-debug": "^0.2.0",
        "phpmailer/phpmailer": "^6.6",
        "phpoffice/phpspreadsheet": "^1.23",
        "pimple/pimple": "^3.4",
        "robmorgan/phinx": "^0.12.7",
        "slim/psr7": "^1.3",
        "slim/slim": "4.*",
        "vlucas/phpdotenv": "^5.3"
    },
    "scripts": {
        "phinx": "phinx"
    }
}
